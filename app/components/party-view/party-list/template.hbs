<div class="list">
  {{list.name}}
  <button class="btn btn-xs btn-danger" {{action 'delete'}}>
    X
  </button>
  <button class="btn btn-xs" {{action 'edit'}}>
    Edit
  </button>

  {{#each list.item_instance as |instance|}}
     {{instance.id}}
     {{#each instance.item as |item|}}
          {{item.name}}
     {{/each}}
  {{/each}}

</div>
